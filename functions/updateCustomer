exports.updateCustomer = (req, res) => {
    var customer = req.body.resource.obj;

    res.status(200).json({
        actions:[{
            action:"setCustomerGroup",
            customerGroup:{
                id: "5e736bb4-1fab-426a-8d60-0214f08cee4c",
                typeId:"customer-group"
            }
        }]
    })
};
